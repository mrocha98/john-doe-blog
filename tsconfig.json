{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "ESNext",                       
    "module": "ES2020",
    "moduleResolution": "Node",
    "noEmit": true,
    "allowJs": true,
    "jsx": "preserve",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}
